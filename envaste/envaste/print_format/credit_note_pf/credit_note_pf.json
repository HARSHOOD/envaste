 "css": "table, th, td {\n  border:1px solid black;\n  color:black;\n  font-size:12px;\n  border-bottom:0px;\n    border-collapse: collapse;\n\n}\n.print-format th{\n      color:black !important;\n}\n.print-format td, .print-format th {\n    padding: 8px !important;\n}\np{\n     font-size:12px;\n}\n.container-3{\n    display: flex;\n    justify-content: space-between;\n    \n}\n .flex-one{\n    border: 1px solid black;\n    text-align: center;\n    padding: 22px;\n    width: 200px;\n    height: 80px;\n    font-size: 12px;\n}\n\n.container1{\n    width:99%;\n}",
